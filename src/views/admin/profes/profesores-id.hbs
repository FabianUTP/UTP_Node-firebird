        <h1>Detalles de {{NOMBREPROFESOR}}</h1>
      </div>

     <form action="" method="post">
        <div class="row g-4">
          <div class="col-md-4">
            <label for="clave" class="form-label">Nombre</label>
            <input required placeholder="nombre" type="text" name="nombre" class="form-control"
            value="{{NOMBREPROFESOR}}" />
          </div>

    
    
          <div class="col-md-4">
            <label for="clave" class="form-label">Género</label>
            <input required placeholder="genero" type="text" name="genero" class="form-control"
            value="{{GENERO}}" />
          </div>

          
          <div class="col-md-4">
            <label for="clave" class="form-label">Cédula Profecional</label>
            <input required placeholder="cedula" type="text" name="cedula" class="form-control"
            value="{{CEDULA_FISCAL}}" />
          </div>

          
          <div class="col-md-4">
            <label for="clave" class="form-label">Clave Ciudadana</label>
            <input required placeholder="clave ciudadana" type="text" name="clave_ciudadana" class="form-control"
            value="{{CLAVE_CIUDADANA}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Estado Civil</label>
            <input required placeholder="est civil" type="text" name="estado_civil" class="form-control"
            value="{{ESTADO_CIVIL}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Fecha de Nacimiento</label>
            <input required placeholder="fech nac" type="text" name="nacimiento" class="form-control"
            value="{{FECHA_NACIMIENTO}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Lugar de Nacimiento</label>
            <input required placeholder="lug nac" type="text" name="lugar_nacimiento" class="form-control"
            value="{{LUGAR_NACIMIENTO}}" />
