          <div class="col-md-4">
            <label for="clave" class="form-label">Domicilio</label>
            <input required placeholder="dom" type="text" name="domicilio" class="form-control"
            value="{{DOMICILIO}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Código Postal</label>
            <input required placeholder="cp" type="text" name="cp" class="form-control"
            value="{{CP}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Ciudad</label>
            <input required placeholder="ciu" type="text" name="ciudad" class="form-control"
            value="{{CIUDAD}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Estado</label>
            <input required placeholder="edo" type="text" name="estado" class="form-control"
            value="{{ESTADO}}" />
          </div>

          <div class="col-md-4">
            <label for="clave" class="form-label">Telefono</label>
            <input required placeholder="CEL" type="text" name="telefono" class="form-control"
            value="{{CELULAR}}" />
          </div>
<div class="pt-3 text-center">
        <button class="btn btn-primary">Guardar</button>
      </div>
          
              
</section>
